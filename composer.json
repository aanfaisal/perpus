{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "algolia/algoliasearch-client-php": "^1.23",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "bratao/prediction-io": "1.*@dev",
        "consoletvs/charts": "^4.6",
        "elasticsearch/elasticsearch": "^5.0",
        "intervention/image": "^2.4",
        "irazasyed/telegram-bot-sdk": "^2.0",
        "laravel/framework": "5.4.27",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "pyaesone17/active-state": "1.1.1",
        "sentry/sentry-laravel": "^0.7.0",
        "tamayo/laravel-scout-elastic": "^3.0",
        "yajra/laravel-datatables-oracle": "^7.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
